{
  "name": "nl.e-missions",
  "version": "1.0.0",
  "description": "E-Missions informative website, built with 11ty",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "DEBUG=Eleventy* npm run dev",
    "dev": "run-s clean eleventy:dev --print-label",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production npx @11ty/eleventy",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yoeran Luteijn",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babel-loader": "^8.2.2",
    "clean-css": "^5.1.2",
    "critical": "^4.0.1",
    "cssesc": "^3.0.0",
    "date-fns": "^2.25.0",
    "del-cli": "^4.0.1",
    "focus-trap": "^6.3.0",
    "focus-visible": "^5.2.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.0.4",
    "memfs": "^3.2.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "svg-sprite": "^1.5.0",
    "webpack": "^5.28.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
